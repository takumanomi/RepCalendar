<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".number">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/number" />

    <Button
        android:id="@+id/secretbutton4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintStart_toStartOf="@+id/secretbutton1"
        app:layout_constraintTop_toBottomOf="@+id/secretbutton1" />

    <Button
        android:id="@+id/secretbutton5"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton4"
        tools:layout_editor_absoluteX="155dp" />

    <Button
        android:id="@+id/secretbutton6"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton5"
        app:layout_constraintEnd_toEndOf="@+id/secretbutton3" />

    <Button
        android:id="@+id/secretbutton2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton1"
        tools:layout_editor_absoluteX="161dp" />

    <Button
        android:id="@+id/secretbutton1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="144dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/secretbutton3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton2"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/secretbutton7"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="50dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintStart_toStartOf="@+id/secretbutton4"
        app:layout_constraintTop_toBottomOf="@+id/secretbutton4" />

    <Button
        android:id="@+id/secretbutton8"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton7"
        tools:layout_editor_absoluteX="161dp" />

    <Button
        android:id="@+id/secretbutton9"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:backgroundTint="#00000000"
        android:backgroundTintMode="src_in"
        android:onClick="onNumberButtonTapped"
        android:text="Button"
        app:layout_constraintBaseline_toBaselineOf="@+id/secretbutton8"
        app:layout_constraintEnd_toEndOf="@+id/secretbutton6" />

</androidx.constraintlayout.widget.ConstraintLayout>